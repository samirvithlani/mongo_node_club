<!DOCTYPE html>
<html>

<head></head>

<body>
    <table border="2">
        <thead>
            <th>Name</th>
            <th>Email</th>
            <th>Age</th>
            <th>ACTION</th>
        </thead>
        <tbody>
            {{#each list}}
            <tr>
                <td>{{this.name}}</td>
                <td>{{this.email}}</td>
                <td>{{this.age}}</td>
                <td><a href="/getdatabyid/{{this._id}}">DETAIL</a></td>
                <td><a href="/delete/{{this._id}}">DELETE</a></td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</body>


</html>